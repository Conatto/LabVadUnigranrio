@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="content-language" content="pt-br" />
    <meta name="author" content="Conatto" />
    <meta name="description" content="Projeto de uma aplicação web para programação e teste de experimentos Arduino por acesso remoto" />
    <meta name="keywords" content="aplicação web, programação, arduino, Arduino, labvad, LabVad, Unigranrio, laboratório virtual, laboratório remoto" />
    <meta name="robots" content="Index, follow" />

    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content")
    @Scripts.Render("~/bundles/modernizr")
</head>

<body>
    <div>
        @RenderBody()
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
